<div class="container">
  <div class='text-center m-4'>
    <h1>My Bookings</h1>
  </div>
  <div class="row justify-content-center">
      <div class="card-racket-container">
        <% @bookings.each do |booking| %>
          <div class="card-racket m-2">
            <% if booking.racket.photo.attached? %>
              <%= cl_image_tag booking.racket.photo.key, height: 300, width: 400, crop: :fill, alt: "alttext" %>
            <% end %>
          <div class="card-racket-infos">
            <div>
              <p><%= booking.start_date %></p>
              <p><%= booking.end_date %></p>
            </div>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>
