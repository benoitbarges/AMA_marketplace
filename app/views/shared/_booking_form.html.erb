<%= simple_form_for([@racket, @booking]) do |f| %>
  <%= f.input :start_date, label: "From:", as: :string, required: false,  input_html: { id: "start_date" } %>
  <%= f.input :end_date, label: "To:", as: :string, required: false, input_html: { id: "end_date" } %>
  <%= f.submit class: "btn btn-primary"%>
<% end %>
